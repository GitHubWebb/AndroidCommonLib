package com.framelibrary.widget.audio;

import java.util.Arrays;

/**
 * 通知栏音频实体信息
 *
 * @author wangwx
 */
public class NotifiMusicBean {
    private String id;
    private String title;
    private byte[] notifiImgByte;
    private String fieldName;

    public NotifiMusicBean() {
    }

    public NotifiMusicBean(String id, String title, byte[] notifiImgBmp, String fieldName) {
        this.id = id;
        this.title = title;
        this.notifiImgByte = notifiImgBmp;
        this.fieldName = fieldName;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public byte[] getNotifiImgByte() {
        return notifiImgByte;
    }

    public void setNotifiImgByte(byte[] notifiImgByte) {
        this.notifiImgByte = notifiImgByte;
    }

    public String getFieldName() {
        return fieldName;
    }

    public void setFieldName(String fieldName) {
        this.fieldName = fieldName;
    }

    @Override
    public String toString() {
        return "NotifiMusicBean{" +
                "id='" + id + '\'' +
                ", title='" + title + '\'' +
                ", fieldName='" + fieldName + '\'' +
                '}';
    }
}
