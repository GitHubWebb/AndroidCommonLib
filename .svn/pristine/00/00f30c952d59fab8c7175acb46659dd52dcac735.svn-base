package com.framelibrary.event;

/**
 * tab签改变event
 */

public class TabUpdateEvent {

    private final int tabPosition;

    private final int tabChildPosition;

    public TabUpdateEvent(int tabPosition, int tabChildPosition) {
        this.tabPosition = tabPosition;
        this.tabChildPosition = tabChildPosition;
    }

    public int getTabPosition() {
        return tabPosition;
    }

    public int getTabChildPosition() {
        return tabChildPosition;
    }
}
